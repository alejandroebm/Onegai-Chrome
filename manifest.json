{
  "name": "Onegai",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Video Scrapper",
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "http://*/", "https://*/"],

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  }
}
